{
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "make build && vite build",
    "serve": "vite preview",
    "watch": "run-p watch:web watch:rust",
    "watch:web": "yarn dev",
    "watch:rust": "chokidar 'src/**/*.rs' -t 1000 -c 'make build'"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.2",
    "vite": "^2.4.2"
  },
  "packageManager": "yarn@3.1.0-rc.5"
}
